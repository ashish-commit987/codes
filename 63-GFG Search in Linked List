/* Node of a linked list
  class Node {
   int data;
    Node next;
    Node(int d)  { data = d;  next = null; }
}
*/
class Solution {
    static boolean searchKey(int n, Node head, int key) {
        // Code here
        //Node newNode=new Node(d);
        Node temp=head;
        
        if(head==null){
            return false;
        }
        while(temp!=null){
            if(temp.data==key){
                return true;
            }
            else {
                temp=temp.next;
            }
        }
        return false;
    }
}
