class Solution {
    public boolean canJump(int[] nums) {
        int n=nums.length;
        int steps=0;
        for(int i=0;i<n;i++){
            if(steps<nums[i]){
                steps=nums[i];
            }
            if(steps==0 && (i+1)!=n){
                return false;
            }
            steps--;
        }
        return true;
    }
}
https://leetcode.com/problems/jump-game/
