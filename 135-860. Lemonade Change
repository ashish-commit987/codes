class Solution {
    public boolean lemonadeChange(int[] bills) {
       int Tc=0;
       int Fc=0;
       for(int i=0;i<bills.length;i++){
        if(bills[i]==5){
            Fc++;
        }
        else if(bills[i]==10){
            if(Fc>0){
                Tc++;
                Fc--;
            }
            else{
                return false;
                //break;
            }
            
        }
        else{
            if(Fc>0 && Tc>0){
                Fc--;
                Tc--;
            }
            else if(Fc>=3){
                Fc-=3;
            }
            else{
                return false;
            }
        }
       } 
       return true;
    }
}
https://leetcode.com/problems/lemonade-change/
